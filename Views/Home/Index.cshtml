@using query.Entity; 
 
@model List<Row> 
 
<h1>Welcome to DB</h1> 
 
<table class="table"> 
  <thead> 
    <tr> 
      <th scope="col">FullName</th> 
      <th scope="col">Username</th> 
    </tr> 
  </thead> 
  @foreach (var user in Model) 
  { 
    <tbody> 
        <tr> 
        <td>@user.Fullname</td> 
        <td>@user.Phone</td> 
        </tr> 
    </tbody>      
  } 
</table>